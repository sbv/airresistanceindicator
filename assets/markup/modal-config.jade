                    div(class="form-group row")
                    div(class="form-group row")
                        label(for="shownumbers" class="col-sm-4 form-control-label") Show numbers
                        div(class="col-sm-8")
                            input(type="checkbox" style="margin-top: 10px;" class="form-control" id="shownumbers" name="shownumbers")

                    div(class="form-group row")
                        label(for="debuglogging" class="col-sm-4 form-control-label") Log position (debugging)
                        div(class="col-sm-8")
                            input(type="checkbox" style="margin-top: 10px;" class="form-control" id="debuglogging" name="debuglogging")


